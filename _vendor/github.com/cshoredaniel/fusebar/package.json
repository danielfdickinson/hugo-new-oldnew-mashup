{
  "name": "fusebar",
  "author": {
    "name": "Daniel F. Dickinson",
    "email": "cshored@thecshore.com",
    "url": "https://www.thecshore.com"
  },
  "version": "0.2.2",
  "description": "Private website search for browsers using Fuse.js as backend",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/cshoredaniel/fusebar"
  },
  "homepage": "https://www.thecshore.com/projects/fusebar",
  "keywords": [
    "fuzzy",
    "bitap",
    "search",
    "browser",
    "website",
    "privacy-by-design"
  ],
  "main": "dist/fusebar.js",
  "scripts": {
    "clean": "rimraf ./dist",
    "pretest": "npm run build",
    "test": "jest",
    "prelint": "npm run clean",
    "prebuild": "npm run lint",
    "predev": "npm run prebuild",
    "lint": "eslint --max-warnings 0 src test",
    "build": "node scripts/build.main.js",
    "dev": "TARGET=umd-dev concurrently -k \"rollup --watch --config scripts/config.js --environment TARGET=umd-dev\" \"browser-sync start --config bs-config.js\"",
    "dev:cjs": "TARGET=commonjs concurrently -k \"rollup --watch --config scripts/config.js --environment TARGET=commonjs\" \"browser-sync start --config bs-config.js\"",
    "dev:esm": "TARGET=esm-dev concurrently -k \"rollup --watch --config scripts/config.js --environment TARGET=esm-dev\" \"browser-sync start --config bs-config.js\""
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.5",
    "@babel/preset-env": "^7.8.7",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@rollup/plugin-replace": "^2.3.2",
    "browser-sync": "^2.26.7",
    "concat": "^1.0.3",
    "concurrently": "^5.1.0",
    "jsdom": "^16.2.2",
    "rimraf": "^2.6.3",
    "rollup": "^2.7.3",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-terser": "^5.3.0"
  },
  "engines": {
    "node": ">=10"
  },
  "files": [
    "dist/"
  ],
  "dependencies": {
    "fuse.js": "^5.2.3"
  }
}
